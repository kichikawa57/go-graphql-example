# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar UUID
scalar Date

scalar UserEmail

enum UserStatus {
  IN_PROGRESS
  COMPLETED
}

type User {
  id: UUID!
  email: UserEmail!
  status: UserStatus!
  age: Int!
}

input CreateUserInput {
  email: UserEmail!
  status: UserStatus!
  age: Int!
}

type Query {
  getUserList: [User!]!
}

type Mutation {
  createUser(input: CreateUserInput!): User!
}
